Cell["\<\
This Demonstration shows three different ways of applying the de Casteljau \
algorithm to a tensor-product B\[EAcute]zier surface.\
\>", "Text"]
}, Closed]],

Cell[CellGroupData[{

Cell["Details", "Section",
 CellFrameColor->RGBColor[
  0.6449835965514611, 0.758632791638056, 0.2516823071641108],
 FontColor->RGBColor[
  0.6449835965514611, 0.758632791638056, 0.2516823071641108]],

Cell[TextData[{
 "A polynomial B\[EAcute]zier surface ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]], "InlineMath"],
 " in tensor-product form is described by the formula"
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "(", 
    RowBox[{
     FormBox[
      RowBox[{
       RowBox[{"u", ",", "v"}], ")"}],
      TraditionalForm], "=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "n"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "p"], " ", 
       RowBox[{
        RowBox[{
         SubscriptBox["B", 
          RowBox[{"i", ",", "n"}]], "(", "u", ")"}], 
        RowBox[{
         SubscriptBox["B", 
          RowBox[{"j", ",", "p"}]], "(", "v", ")"}], " ", 
        SubscriptBox["R", 
         RowBox[{"i", ",", "j"}]]}]}]}]}]}], TraditionalForm]], "InlineMath"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", 
    RowBox[{"i", ",", "j"}]], TraditionalForm]], "InlineMath"],
 " are the control points of ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]], "InlineMath"],
 "; they form the control net of ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 "Recall that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["B", 
      RowBox[{"i", ",", "n"}]], "(", "u", ")"}], " ", "=", " ", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"n"},
        {"i"}
       }], ")"}], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        RowBox[{
         RowBox[{"1", "-", "u"}], ")"}], 
        RowBox[{"n", "-", "i"}]], 
       SuperscriptBox["u", "i"]}]}]}]}], TraditionalForm]], "InlineMath"],
 " is the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["i", "th"], TraditionalForm]], "InlineMath"],
 " Bernstein basis polynomial of degree ",
 Cell[BoxData[
  StyleBox["n", "TI"]], "InlineFormula"],
 " in ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "0"}], ",", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 "In order to determine the point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "(", 
    RowBox[{
     SubscriptBox["u", 
      RowBox[{"0", ","}]], 
     SubscriptBox["v", "0"]}], ")"}], TraditionalForm]], "InlineMath"],
 " (for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "0"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "0"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), the de Casteljau algorithm can be used. For a polynomial B\[EAcute]zier \
curve "
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], "n"], 
     RowBox[{
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"i", ",", "n"}]], "(", "t", ")"}], " ", 
      SubscriptBox["P", "i"]}]}]}], TraditionalForm]], "InlineMath"],
 ", (for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", " ", "\[Element]", " ", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]], "InlineMath"],
 "),"
}], "Text"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "i"], TraditionalForm]], "InlineMath"],
 " are the control points, this algorithm calculates a current point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"C", "(", 
    SubscriptBox["t", "0"]}], TraditionalForm]], "InlineMath"],
 ") by applying the following recurrence formula:"
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "0"], "=", 
    SubscriptBox["P", "i"]}], TraditionalForm]], "InlineMath"],
 ", for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 ","
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "k"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SubscriptBox["t", "0"]}], ")"}], 
      SubsuperscriptBox["P", "i", 
       RowBox[{"k", "-", "1"}]]}], "+", 
     RowBox[{
      SubscriptBox["t", "0"], 
      SubsuperscriptBox["P", 
       RowBox[{"i", "+", "1"}], 
       RowBox[{"k", "-", "1"}]]}]}]}], TraditionalForm]], "InlineMath"],
 ", for",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"i", "=", "0"}]}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "k"}], TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell["Finally, we obtain", "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", 
     SubscriptBox["t", "0"], ")"}], "=", 
    SubsuperscriptBox["P", "0", "n"]}], TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 "In order to determine the point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "(", 
    RowBox[{
     SubscriptBox["u", 
      RowBox[{"0", ","}]], 
     SubscriptBox["v", "0"]}], ")"}], TraditionalForm]], "InlineMath"],
 " (for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "0"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "0"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), the de Casteljau algorithm can be applied to the surface in the \
following three ways:"
}], "Text"],

Cell[TextData[{
 "First in the ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]], "InlineMath"],
 " direction: by de Casteljau, we first determine the points "
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["R", 
     RowBox[{"*", 
      RowBox[{",", "j"}]}]], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], "n"], " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"i", ",", "n"}]], "(", 
       SubscriptBox["u", "0"], ")"}], " ", 
      SubscriptBox["R", 
       RowBox[{"i", ",", "j"}]]}]}]}], TraditionalForm]], "InlineMath"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell["Next, the algorithm is used to compute ", "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", 
     RowBox[{
      SubscriptBox["u", 
       RowBox[{"0", ","}]], 
      SubscriptBox["v", "0"]}], ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "p"], " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"j", ",", "p"}]], "(", 
       SubscriptBox["v", "0"], ")"}], " ", 
      SubscriptBox["R", 
       RowBox[{"*", 
        RowBox[{",", "j"}]}]]}]}]}], TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 "Second in the ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]], "InlineMath"],
 " direction: analogously, we first determine the points "
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["R", 
     RowBox[{"i", ",", "*"}]], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "p"], " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"j", ",", "p"}]], "(", 
       SubscriptBox["v", "0"], ")"}], " ", 
      SubscriptBox["R", 
       RowBox[{"i", ",", "j"}]]}]}]}], TraditionalForm]], "InlineMath"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell["Then we compute ", "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", 
     RowBox[{
      SubscriptBox["u", 
       RowBox[{"0", ","}]], 
      SubscriptBox["v", "0"]}], ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], "n"], " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"i", ",", "n"}]], "(", 
       SubscriptBox["u", "0"], ")"}], " ", 
      SubscriptBox["R", 
       RowBox[{"i", ",", "*"}]]}]}]}], TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 "Simultaneously in the ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]], "InlineMath"],
 " directions: this method is easier to use in the case where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "p"}], TraditionalForm]], "InlineMath"],
 ":"
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["R", 
     RowBox[{"i", ",", "j"}], "0"], "=", 
    SubscriptBox["R", 
     RowBox[{"i", ",", "j"}]], " "}], TraditionalForm]], "InlineMath"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 ","
}], "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["R", 
     RowBox[{"i", ",", "j"}], "k"], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{"1", "-", 
          SubscriptBox["v", "0"]}], 
         SubscriptBox["v", "0"]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubsuperscriptBox["R", 
          RowBox[{"i", ",", "j"}], 
          RowBox[{"k", "-", "1"}]], 
         SubsuperscriptBox["R", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", "j"}], 
          RowBox[{"k", "-", "1"}]]},
        {
         SubsuperscriptBox["R", 
          RowBox[{"i", ",", 
           RowBox[{"j", "+", "1"}]}], 
          RowBox[{"k", "-", "1"}]], 
         SubsuperscriptBox["R", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", 
           RowBox[{"j", "+", "1"}]}], 
          RowBox[{"k", "-", "1"}]]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"1", "-", 
          SubscriptBox["u", "0"]}]},
        {
         SubscriptBox["u", "0"]}
       }], ")"}]}]}], TraditionalForm]], "InlineMath"],
 ", for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "k"}], TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "k"}], TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", "1"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 ","
}], "Text"],

Cell["yielding ", "Text"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", 
     RowBox[{
      SubscriptBox["u", 
       RowBox[{"0", ","}]], 
      SubscriptBox["v", "0"]}], ")"}], "=", 
    SubsuperscriptBox["R", 
     RowBox[{"0", ",", "0"}], "n"]}], TraditionalForm]], "InlineMath"],
 "."
}], "Text"],

Cell[TextData[{
 " If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "<", "p"}], TraditionalForm]], "InlineMath"],
 ", the above procedure is applied in order to calculate ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["R", 
    RowBox[{"i", ",", "j"}], "k"], TraditionalForm]], "InlineMath"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", "0"}], TraditionalForm]], "InlineMath"],
 " to ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 ", and next the ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]], "InlineMath"],
 " direction method is used to compute the remaining iteration levels. The \
case ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", ">", "p"}], TraditionalForm]], "InlineMath"],
 " is treated analogously."
}], "Text"]
